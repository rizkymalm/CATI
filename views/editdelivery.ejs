<!DOCTYPE html>
<html>
<head>
	<!-- <title>Dashboard</title> -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700&display=swap" rel="stylesheet">
	<link rel="stylesheet" href="<%= baseurl() %>css/style.css">
    <link rel="stylesheet" href="<%= baseurl() %>css/mystyle.css">
    <link rel="stylesheet" href="<%= baseurl() %>css/style_button.css">
	<link rel="stylesheet" href="<%= baseurl() %>css/component.css">
    <link rel="stylesheet" href="<%= baseurl() %>css/responsive.css">
    <link rel="stylesheet" href="<%= baseurl() %>plugin/fileinput/component.css">
    <link rel="stylesheet" href="<%= baseurl() %>plugin/fileinput/normalize.css">
</head>
<body>
<div class="wrapper">
	<div class="lSide">
		<% include ./partials/sidebar %>
	</div>
	<div class="rSide desk">
		<div class="head gradientr desk">
			<% include ./partials/topdesk %>
		</div>
		<div class="mainbox size-std column">
			<div class="title-content size-std" style="background-color: #68baf9; width: 100%; padding: 10px 10px; margin: 10px 5px; color: #ffffff;">
				DASHBOARD > Project > Upload - <%= login.nameses %>
            </div>
            <div class="cols-12 column containt" style="margin: 10px 0; background-color: #ffffff;">
                <div class="headtitle trans">
                    <p class="nopad size-head4" style="padding: 5px 15px; position: relative;">
                        Upload Services Files
                    </p>
                    <ul class="tab-image-setting nopad">
                        <li><img src="<%= baseurl() %>images/icon/arrow-up.png" class="btn-tab-toggle delay" href="assigntome"></li>
                    </ul>
                </div>
				<div class="cols-12" style="padding: 35px 0;">
				<form action="<%= baseurl() %>delivery/update/<%= delivery[0].id_exceldlv %>/<%= delivery[0].no_delivery %>" method="POST">
                    <table class="full-length size-std basic-tables unbordered" style="border-right:1px solid #dfe8f1" cellpadding="8" cellspacing="8">
						<tr>
							<td style="text-align: right; width: 40%;">*No Delivery</td>
							<td>
								<input type="text" name="id_delivery" class="frm-std size-std" style="border-radius: 0; text-transform: uppercase;" required="" value="<%= delivery[0].id_delivery %>">
								<% for (showerr of iddelivery_err){ %>
									<%= showerr.error_msg %>
								<% } %>
							</td>
						</tr>
						<tr>
							<td style="text-align: right; width: 40%;">*No Rangka</td>
							<td>
								<input type="text" name="no_rangka" class="frm-std size-std" style="border-radius: 0; text-transform: uppercase;" required="" value="<%= delivery[0].no_rangka %>">
								<% for (showerr of norangka_err){ %>
									<%= showerr.error_msg %>
								<% } %>
							</td>
						</tr>
                        <tr>
							<td style="text-align: right; width: 40%;">*No Mesin</td>
							<td>
								<input type="text" name="no_mesin" class="frm-std size-std" style="border-radius: 0; text-transform: uppercase;" required="" value="<%= delivery[0].no_mesin %>">
							</td>
						</tr>>
							<td style="text-align: right; width: 40%;">*Tipe Kendaraan</td>
							<td>
								<input type="text" name="type_kendaraan" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= delivery[0].type_kendaraan %>">
							</td>
                        </tr>
                        <tr>
							<td style="text-align: right; width: 40%;">*Warna</td>
							<td>
								<input type="text" name="warna" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= delivery[0].warna %>">
							</td>
						</tr>
						<tr>
							<td style="text-align: right; width: 40%;">*Nama STNK</td>
							<td>
								<input type="text" name="nama_stnk" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= delivery[0].nama_stnk %>">
							</td>
						</tr>
						<tr>
							<td style="text-align: right; width: 40%;">*Jenis Kelamin</td>
							<td>
								<input type="text" name="jk" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= delivery[0].jk %>">
							</td>
						</tr>
						<tr>
							<td style="text-align: right; width: 40%;">*Nama User</td>
							<td>
								<input type="text" name="user_name" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= delivery[0].user_name %>">
							</td>
						</tr>
						<tr>
							<td style="text-align: right; width: 40%;">*No Hp</td>
							<td>
								<input type="text" name="no_hp" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= delivery[0].no_hp %>">
							</td>
						</tr>
						<tr>
							<td style="text-align: right; width: 40%;">*Tanggal Service</td>
							<td>
								<input type="datetime-local" name="tgl_delivery" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= moment(delivery[0].tgl_delivery).format('YYYY-MM-DDTHH:MM') %>">
							</td>
						</tr>
						<tr>
							<td style="text-align: right; width: 40%;">*Dealer</td>
							<td>
								<input type="text" name="id_dealer" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= delivery[0].id_dealer %>">
								<% for (showerr of iddealer_err){ %>
									<%= showerr.error_msg %>
								<% } %>
							</td>
						</tr>
						<tr>
							<td style="text-align: right; width: 40%;">*Nama Sales</td>
							<td>
								<input type="text" name="id_sales" class="frm-std size-std" style="border-radius: 0;" required="" value="<%= delivery[0].id_sales %>">
							</td>
						</tr>
						<tr>
							<td colspan="2" align="center"><input type="submit" value="Save Change" class="myButton-flat-green"></td>
						</tr>
					</table>
				</form>	
                </div>
		    </div>
		</div>
	</div>
</div>
</body>
<% include ./partials/footer %>

<link rel="stylesheet" href="<%= baseurl() %>plugin/fileinput/custom-file-input.js">
</html>